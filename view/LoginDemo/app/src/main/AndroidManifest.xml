<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parth.logindemo">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cropwatchicon"
        android:supportsRtl="true"
        android:name=".AppController"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity" />
        <activity android:name=".Register" />
        <activity android:name=".Login_email" />
        <activity android:name=".SensorData" />
        <activity android:name=".Complain" />
        <activity android:name=".Feedback" />
        <activity android:name=".MotorStatus"></activity>
    </application>

</manifest>